#include <stdio.h>
#include <stdlib.h>

typedef struct Lista{
    
    int qtd;
    struct Registro *inicio;

}Lista;

typedef struct Registro{

    int valor;
    struct Registro*prox;

}Registro;

Lista *aloca_lista(){

    Lista *novo;
    novo = (Lista *)malloc(sizeof(Lista));
    novo->qtd = 0;
    novo->inicio = NULL;
    return novo;

}

Registro *aloca_registro(){

    Registro *novo;
    novo = (Registro *)malloc(sizeof(Registro));
    novo->valor = 0;
    novo->prox = NULL;
    return novo;

}