void incluir_no_inicio_2(Lista *lista, int valor){

    Registro *novo;
    novo = aloca_registro();
    novo->valor = valor;

    if(lista->inicio == NULL){

        lista->inicio = novo;
        lista->final = novo;
    
    }else{

        lista->inicio->anterior = novo;
        novo->prox = lista->inicio;
        lista->inicio = novo;

    }

    lista->qtd++;

}